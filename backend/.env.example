# API Keys for LLM models

# Google Gemini (for gemini-2.0-flash-exp, gemini-1.5-pro, etc.)
GEMINI_API_KEY=your_gemini_api_key_here

# DeepSeek (for deepseek-chat)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
# DeepSeek base URL (optional, defaults to their API endpoint)
DEEPSEEK_BASE_URL=https://api.deepseek.com

# OpenAI (optional, if you want to use GPT models)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude (optional)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Default model to use (can be overridden with --model flag)
DEFAULT_MODEL=gemini-2.0-flash-exp
